function v2g() {
  ffmpeg -i "$1" -pix_fmt rgb8 -r 10 "$2" && gifsicle -O3 "$2" -o "$2"
}

# force env vars that are updated in tmux session to update in pane
function tmup() {
  eval $(tmux showenv -s)
  echo "ðŸ¤˜tmux env reloaded..."
}

function jme() {
  jira issue list -a "$(jira me)" -s~Done --columns KEY,SUMMARY,STATUS,PRIORITY
}
